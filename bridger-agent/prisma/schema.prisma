generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Greet {
  id Int @id @default(autoincrement())
}

model Transaction {
  id Int @id @default(autoincrement())
  date DateTime
  bankDetails String
  amountCents Int
  predVendor String?
  pickedVendor String?
  predCategory String?
  pickedCategory String?
}
